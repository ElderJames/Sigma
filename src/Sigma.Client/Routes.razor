@using MudBlazor
<Router AppAssembly="typeof(App).Assembly" >
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="routeData" DefaultLayout="typeof(Sigma.Client.Layouts.BasicLayout)">
            <NotAuthorized>
                <RedirectToLogin />
            </NotAuthorized>
        </AuthorizeRouteView>
        <FocusOnNavigate RouteData="routeData" Selector="h1" />
    </Found>
</Router>

<AntContainer />
<MudThemeProvider />
<MudDialogProvider/>
<MudSnackbarProvider/>